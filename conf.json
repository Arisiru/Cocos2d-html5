{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include" : ["js"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "opts": {
        "template": "node_modules/ink-docstrap/template",
        "encoding": "utf8",
        "destination": "doc-js/",
        "recurse": true,
        "query": "value",
        "private": true,
        "lenient": true
    },
    "templates": {
        "cleverLinks"           : false,
        "monospaceLinks"        : false,
        "systemName"            : "CocosJS",
        "footer"                : "@hyperandroid",
        "copyright"             : "Chukong inc. 2014",
        "navType"               : "vertical",
        "theme"                 : "cerulean",
        "linenums"              : true,
        "collapseSymbols"       : false,
        "inverseNav"            : false,
        "outputSourceFiles"     : true,
        "outputSourcePath"      : true,
        "dateFormat"            : "mm/dd/YYYY",
        "highlightTutorialCode" : false
    }
}